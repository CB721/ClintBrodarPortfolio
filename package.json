{
  "name": "clintbrodarportfolio",
  "version": "1.0.0",
  "description": "Portfolio of projects created / worked on by Clint Brodar",
  "main": "server.js",
  "proxy": "http://0.0.0.0:3001",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install && npm run build && cd ..",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CB721/ClintBrodarPortfolio.git"
  },
  "keywords": [
    "clint",
    "brodar",
    "portfolio",
    "react"
  ],
  "author": "Clint Brodar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CB721/ClintBrodarPortfolio/issues"
  },
  "homepage": "https://github.com/CB721/ClintBrodarPortfolio#readme",
  "dependencies": {
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.1",
    "mongoose-auto-increment": "^5.0.1",
    "path": "^0.12.7"
  }
}
